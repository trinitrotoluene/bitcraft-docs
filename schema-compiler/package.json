{
  "name": "@bitcraft-docs/schema-compiler",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    }
  },
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest --run",
    "test:watch": "vitest",
    "typecheck": "tsc",
    "format:file": "prettier --write",
    "format": "pnpm run format:file '**/*.{ts,tsx,js,jsx,json,md,mdx,yml,yaml}'",
    "lint:file": "eslint --fix",
    "lint": "pnpm run lint:file .",
    "start": "tsx src/index.ts"
  },
  "dependencies": {
    "@bitcraft-docs/schema-parser": "workspace:../schema-parser",
    "commander": "^14.0.2",
    "fast-glob": "^3.3.3",
    "gray-matter": "^4.0.3",
    "markdown-table": "^3.0.4",
    "zod": "^4.2.1"
  }
}
